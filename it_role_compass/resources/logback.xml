<configuration>
  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
  <logger name="smile.classification.RandomForest" level="OFF"/>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>
</configuration>
